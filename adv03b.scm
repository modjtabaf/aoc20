(define (func rstep cstep)
  (define nrows (length input))
  (define ncols (string-length (car input)))
  (define col 0)
  (define total 0)

  (do
    ((row 0 (+ row rstep)))
    ((>= row nrows))
    
    (if (>= col ncols) (set! col (- col ncols)))
    (if (char=? (list-ref (string->list (list-ref input row)) col) #\#)
      (set! total (1+ total))
    )
    (set! col (+ col cstep))
  )
  
  total
)

